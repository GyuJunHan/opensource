# opensourcefrom tkinter import *
from tkinter import messagebox


## 함수 선언 부분 ##
def keyEvent(event):
    messagebox.showinfo("키보드 이벤트", "눌린 키 :  " + (event.keysym))


## 메인 코드 부분 ##
window = Tk()

window.bind("<Up>", keyEvent)
window.bind("<Down>", keyEvent)
window.bind("<Left>", keyEvent)
window.bind("<Right>", keyEvent)





window.mainloop()
